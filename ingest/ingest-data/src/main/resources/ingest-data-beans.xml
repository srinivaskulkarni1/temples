<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	xmlns:context="http://www.springframework.org/schema/context">

	<import resource="classpath*:/data-model-beans.xml" />
	<import resource="classpath*:/common-utils-beans.xml" />

	<bean id="dataloader" class="com.temples.in.ingest_data.DataLoader" />
	<bean id="dbconnection" class="com.temples.in.ingest_data.data_access.DBConnection" />
	<bean id="insertstmtexecutor" class="com.temples.in.ingest_data.data_access.InsertStatementExecutor" />
	<bean id="params" class="com.temples.in.ingest_data.Params" scope="prototype"/>
	<bean id="paramsbuilder" class="com.temples.in.ingest_data.ParamsBuilder" />

	<context:annotation-config />
	<context:component-scan base-package="com.temples.in.ingest_data" />

</beans>